type Flavor {
    Orange
    , Apple
}

type Person {
    name: String
    , age: U8
    , favorite_flavor: Flavor
}

does_like : Person -> Flavor -> Bool;
does_like person flavor = Unit;

main {
    alice = Person { name = "Alice", age = 25, favorite_flavor = Flavor.Orange };
    likes = does_like alice Flavor.Orange;
}
